{
  "expo": {
    "name": "Bhavana",
    "slug": "Bhavana",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.openarchitex.Bhavana"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "sounds": [
            "./assets/timer-sounds/thadobati_d_sharp.wav",
            "./assets/timer-sounds/thadobati_g_sharp.wav",
            "./assets/timer-sounds/manipuri_c_sharp.wav",
            "./assets/timer-sounds/manipuri_a_sharp.wav",
            "./assets/timer-sounds/lingam_c_sharp.wav",
            "./assets/timer-sounds/crystal_bowl.wav",
            "./assets/timer-sounds/bell_sound.wav"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "082a3a53-6396-445f-b350-13de3acee0d7"
      }
    }
  }
}
